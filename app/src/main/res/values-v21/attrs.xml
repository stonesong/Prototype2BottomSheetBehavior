<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension" />
    </declare-styleable>

    <declare-styleable name="BackdropBottomSheetBehavior_Params">
        <attr name="behavior_backdrop_peekHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="Custom_BottomSheetBehavior_Layout">
        <attr name="custom_behavior_peekHeight" format="dimension">
            <enum name="auto" value="-1" />
        </attr>
        <attr name="custom_behavior_hideable" format="boolean" />
        <attr name="custom_behavior_skipCollapsed" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScrollingAppBarLayoutBehavior_Params">
        <attr name="behavior_scrolling_appbar_peek_height" format="dimension"/>
    </declare-styleable>
</resources>